{
  "manifest_version": 2,

  "name": "Fone King",
  "description": "Talk to RepairCMS from Vend",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },
  "omnibox": { "keyword" : "cms" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Search RepairCMS"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
